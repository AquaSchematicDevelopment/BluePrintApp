<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Blue prints</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @holdings.each do |holding| %>
      <tr>
        <td><%= holding.team.name %></td>
        <td><%= holding.blue_prints %></td>
      </tr>
    <% end %>
  </tbody>
</table>
