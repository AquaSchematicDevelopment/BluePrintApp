<% @holdings.each_with_index do |holding, index| %>
  <div class="row <%= index.even? ? 'even-row' : 'odd-row' %>">
    <div class="col-xs-4">
      <p>Team: <%= holding.team.name %></p>
    </div>
    <div class="col-xs-4">
      <p>Blueprints: <%= holding.blue_prints %></p>
    </div>
    <div class="col-xs-4">
      <p><%= link_to 'sell', sell_holding_path(holding) %></p>
    </div>
  </div>
<% end %>
