<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Blue prints</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @holdings.each do |holding| %>
      <tr>
        <td><%= holding.team.name %></td>
        <td><%= holding.blue_prints %></td>
        <tf><%= link_to 'sell', sell_holding_path(holding.team.safe_name) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
