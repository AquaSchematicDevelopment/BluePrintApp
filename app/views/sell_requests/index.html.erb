<p id="notice"><%= notice %></p>

<h1>Listing Sell Requests</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sell_requests.each do |sell_request| %>
      <tr>
        <td><%= sell_request.price %></td>
        <td><%= sell_request.amount %></td>
        <td><%= link_to 'Show', sell_request %></td>
        <td><%= link_to 'Edit', edit_sell_request_path(sell_request) %></td>
        <td><%= link_to 'Destroy', sell_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sell request', new_sell_request_path %>
