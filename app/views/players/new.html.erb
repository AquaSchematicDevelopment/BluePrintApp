<%= form_for(@player) do |form| %>
  <%= form.label 'Username' %>
  <%= form.text_field :name %>
  <%= form.label 'Email' %>
  <%= form.email_field :email %>
  <%= form.label 'Password' %>
  <%= form.password_field :password %>
  <%= form.label 'Confirm Password' %>
  <%= form.password_field :confirm_password %>
<% end %>
